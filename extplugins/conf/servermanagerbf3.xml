<configuration plugin="servermanagerbf3">
    <settings name="commands">
        <!-- This section defines the level required to use the commands
              0 : guest
              1 : user
              2 : regular
              20 : moderator
              40 : admin
              60 : fulladmin
              80 : senioradmin
              100 : superadmin
          -->
        <!-- Also, you can define command aliases by adding it after the command name, separating them with '-'.
            For example if you want to use !ct instead of !changeteam, use the name "changeteam-ct" when
            defining the command level below.
          -->

        <set name="tickets">20</set>
		<set name="settickets">60</set>z
		<set name="autotickets">20</set>
		<set name="autorotation">20</set>
		<set name="setrotation">40</set>
		<set name="autoplayercount">20</set>

    </settings>
	<settings name="settings">
		<!-- Map Rotation settings-->
		<set name="auto_rotation_adjust">True</set>
		<set name="map_rotations">4</set>
		<set name="randomize">True</set>
		<!--Server player settings-->
		<set name="adjust_players">True</set>
		<set name="max_players">64</set>
		<set name="player_counts">0:8,12:16,20:24,28:32,40:48,44:64</set>
		<!--Ticket counts-->
		<set name="auto_tickets_adjust">True</set>
		<set name="breakpoints">5</set>
		<set name="ticket_configs">2</set>
		<set name="max_tickets">80</set>
		<!--Idle kick Settings-->
		<set name="idle_kick_disable">True</set>
		<set name="idle_player_count">10</set>
	</settings>
